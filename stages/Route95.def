p1starty = 0
p1startz = 0
p1facing = 1
 
;--- Player 2 ---
p2startx = 70
p2starty = 0
p2startz = 0
p2facing = -1

;--- Common ---
leftbound  = -1250
rightbound =  1250

[Scaling]
topz     = 0       
botz     = 50      
topscale = 1       
botscale = 1.2 

[Bound]
screenleft = 15
screenright = 15

[StageInfo]
